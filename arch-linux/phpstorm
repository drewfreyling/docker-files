FROM sandrokeil/archlinux:latest
RUN pacman -Sy --noconfirm git openssh vim \
    && pacman -Scc --noconfirm
RUN mkdir -p /opt/phpstorm && curl -L https://download.jetbrains.com/webide/PhpStorm-2018.2.4.tar.gz | tar xzf - --strip-components=1 -C /opt/phpstorm
ENTRYPOINT ["/opt/phpstorm/bin/phpstorm.sh"]
